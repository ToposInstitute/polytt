def nyan : Poly :=
  (ℕ × ℕ)

def tensor : Poly → Poly → Poly :=
  λ P Q → (pq : base P × base Q) × (fib P (fst pq) × fib Q (snd pq))

#normalize base (tensor nyan nyan)
#normalize fib (tensor nyan nyan) (1 , 2)


def id : (P : Poly) → P ⇒ P :=
  λ P → λ a⁺ a⁻ ⇝
    return a⁺ ⇜ a⁻

#print (λ P → P) : Poly → Poly

