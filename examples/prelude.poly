def compose : Poly → Poly → Poly :=
  λ P Q → (pf : (p : base P) × (fib P p → base Q)) × (i : fib P (fst pf)) × fib Q ((snd pf) i)

def tensor-to-compose : (P : Poly) → (Q : Poly) → tensor P Q ⇒ compose P Q :=
  λ P Q → λ pq⁺ pq⁻ ⇝
    return (fst pq⁺ , λ _ → snd pq⁺) ⇜ pq⁻

#normalize tensor-to-compose

def Lens : (S T A B : Type) → Type := λ S T A B → { .get : S → A, .set : S → B → T }

def compose : (A B C : Type) → (B → C) → (A → B) → A → C :=
  λ _ _ _ f g a → f (g a)

#normalize Lens
